{
  "homepage": "https://annabasenyuk.github.io/task_planner_app/",
  "name": "task_planner_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mate-academy/ts-config": "^1.0.5",
    "@react-hook/media-query": "^1.1.1",
    "@types/node": "^16.18.68",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "classnames": "^2.3.2",
    "date-fns": "^3.3.1",
    "gh-pages": "^6.1.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "sass": "^1.69.5",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build $GH_TOKEN",
    "start": "react-scripts start --url=/task_planner_app",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier . --ignore-path .gitignore --write",
    "lint": "eslint . --ignore-path .eslintignore --fix",
    "fix-style": "npm run lint",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@mate-academy/eslint-config-react-typescript": "^1.0.13",
    "@mate-academy/stylelint-config": "^0.0.12",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run fix-style"
  }
}
